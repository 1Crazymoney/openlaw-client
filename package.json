{
  "name": "openlaw",
  "version": "0.1.7",
  "description": "JavaScript library for integrating the OpenLaw protocol to store, edit, and manage smart legal agreements.",
  "keywords": [
    "openlaw",
    "legal",
    "law",
    "contract",
    "agreement",
    "blockchain",
    "ethereum",
    "smart contract"
  ],
  "sideEffects": false,
  "main": "dist/cjs/openlaw.js",
  "module": "dist/esm/index.js",
  "files": [
    "dist/cjs",
    "dist/esm",
    "dist/umd"
  ],
  "watch": {
    "test": {
      "patterns": [
        "js/src"
      ],
      "extensions": [
        "js",
        "scala"
      ]
    }
  },
  "scripts": {
    "flow": "flow",
    "build": "webpack --progress --colors --config webpack.dev.js && npm run build:esm",
    "build:esm": "rimraf dist/esm && npm run build:esm:apiclient && npm run build:esm:openlaw && npm run build:esm:index",
    "build:esm:openlaw": "npx cpy 'target/scala-2.12/client.js' 'dist/esm/' && npx terser dist/esm/client.js -cm --module -o dist/esm/openlaw.js && rimraf dist/esm/client.js",
    "build:esm:apiclient": "npx babel js/src/APIClient.js --out-dir dist/esm/ --copy-files --presets=minify",
    "build:esm:index": "npx cpy 'js/src/index.js' 'dist/esm/'",
    "build_prod": "flow && webpack --config webpack.prod.js && npm run build:esm",
    "watch": "flow && webpack --progress --colors --watch --config webpack.dev.js",
    "lint": "eslint --c .eslintrc.json 'js/src/**/*.js' --fix",
    "format": "prettier --config .prettierrc 'js/src/**/*.js' --write",
    "prepublishOnly": "run-s build_prod use:npmReadme",
    "postpublish": "npm run use:gitReadme",
    "release": "np",
    "use:npmReadme": "mv 'README.md' 'git.README.md' && mv 'npm.README.md' 'README.md'",
    "use:gitReadme": "mv 'README.md' 'npm.README.md' && mv 'git.README.md' 'README.md'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/openlawteam/openlaw-client.git"
  },
  "author": "OpenLaw Team <opensource@openlaw.io>",
  "license": "Apache-2.0",
  "bugs": "https://github.com/openlawteam/openlaw-client/issues",
  "homepage": "https://openlaw.io",
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.2.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-flow": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "babel-preset-minify": "^0.5.0",
    "clean-webpack-plugin": "^1.0.0",
    "codacy-coverage": "^3.3.0",
    "cpy-cli": "^2.0.0",
    "eslint": "^5.9.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-flowtype": "^3.2.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-security": "^1.4.0",
    "flow-bin": "^0.87.0",
    "flow-watch": "^1.1.4",
    "np": "^3.0.4",
    "npm-run-all": "^4.1.5",
    "npm-watch": "^0.4.0",
    "prettier": "1.14.3",
    "rimraf": "^2.6.2",
    "terser": "^3.11.0",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2",
    "webpack-merge": "^4.1.4"
  },
  "dependencies": {
    "@babel/runtime": "^7.2.0",
    "axios": "^0.18.0",
    "query-string": "^6.2.0"
  }
}
